// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root@127.0.0.1:3309/test" 
}

model users {
  id    Int @default(autoincrement()) @id
  email String @unique
  name  String
}